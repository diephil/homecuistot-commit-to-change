    "csv-parse": "^6.1.0",
    "drizzle-orm": "^0.45.1",
    "fastest-levenshtein": "^1.0.16",
    "lucide-react": "^0.562.0",
    "next": "16.1.3",
    "next-themes": "^0.4.6",
    "openai": "^6.17.0",
    "opik": "^1.9.89",
    "opik-gemini": "^1.9.98",
    "opik-openai": "^1.10.2",
    "opik-vercel": "^1.9.89",
    "postgres": "^3.4.8",
    "react": "19.2.3",
    "react-dom": "19.2.3",

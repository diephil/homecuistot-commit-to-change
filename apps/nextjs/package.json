    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "glob": "^13.0.0",
    "shadcn": "^3.7.0",
    "supabase": "^2.72.8",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.17"
  }

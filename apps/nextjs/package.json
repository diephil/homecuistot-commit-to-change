{
  "name": "homecuistot-hackathon",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:db": "vitest run tests/db",
    "db:generate": "drizzle-kit generate",
    "db:generate:manual": "tsx src/db/generate-manual-migration.ts",
    "db:migrate": "tsx --env-file=.env.local src/db/migrate.ts",
    "db:migrate:prod": "tsx --env-file=.env.prod src/db/migrate.ts",
    "db:baseline:prod": "tsx --env-file=.env.prod src/db/baseline.ts",
    "db:status": "tsx --env-file=.env.local src/db/status.ts",
    "db:status:prod": "tsx --env-file=.env.prod src/db/status.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "prompt:all": "tsx --env-file=.env.local scripts/register-prompts.ts",
    "prompt:all:prod": "tsx --env-file=.env.prod scripts/register-prompts.ts",
    "dataset:register": "tsx --env-file=.env.local scripts/register-datasets.ts",
    "dataset:register:prod": "tsx --env-file=.env.prod scripts/register-datasets.ts",
    "eval": "tsx --env-file=.env.local scripts/run-evaluation.ts",
    "eval:prod": "tsx --env-file=.env.prod scripts/run-evaluation.ts",
    "eval:all": "tsx --env-file=.env.local scripts/run-evaluations.ts",
    "eval:all:prod": "tsx --env-file=.env.prod scripts/run-evaluations.ts",
    "users": "tsx --env-file=.env.local scripts/list-users.ts",
    "users:prod": "tsx --env-file=.env.prod scripts/list-users.ts",
    "usage": "tsx --env-file=.env.local scripts/llm-usage-today.ts",
    "usage:prod": "tsx --env-file=.env.prod scripts/llm-usage-today.ts"
  },
  "dependencies": {
